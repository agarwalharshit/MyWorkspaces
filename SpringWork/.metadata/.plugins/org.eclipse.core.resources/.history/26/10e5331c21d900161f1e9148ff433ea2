package com.sjsu;

import java.util.*;

public class HelloWorld {
	private String message;	
	private int num;
	private Answers a;
	
	public HelloWorld(int num, String message, Answers  a){
		this.num=num;
		this.message=message;
		this.a=a;
	}
	public HelloWorld(){
	}
	
	
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}

	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public Map<String, Answers> getA() {
		return a;
	}
	public void setA(Map<String, Answers> a) {
		this.a = a;
	}
	public void print(){
		System.out.println(num+"  "+message+"Answer No= "+a.get("2").ansNo+" Answer="+a.get("2").ans);
	}
	
	
	
	
	
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
