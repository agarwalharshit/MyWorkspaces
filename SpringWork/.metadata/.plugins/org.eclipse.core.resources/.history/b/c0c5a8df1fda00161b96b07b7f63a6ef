package com.hibernate;

public class Player {
int id;
String name;
int teamId;
int age;
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getTeamId() {
	return teamId;
}
public void setTeamId(int teamId) {
	this.teamId = teamId;
}
public int getAge() {
	return age;
}
public void setAge(int age) {
	this.age = age;
}


}
